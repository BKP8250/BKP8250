row1=[" | |","| |","| |"]
row2=[" | |","| |","| |"]
row3=[" | |","| |","| |"]
map=[row1,row2,row3]
print(f"{row1}\n{row2}\n{row3}")
position=input("where you want to put the input")
vertical=int(position[1])
horizontal=int(position[0])
map[horizontal-1][vertical-1]='X'
print(f"{row1}\n{row2}\n{row3}")


